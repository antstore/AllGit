<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
   "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="org.walkmod:walkmod-sonar-plugin:AddCurlyBrackets"
		class="org.walkmod.sonar.visitors.AddCurlyBrackets"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:AddSwitchDefaultCase"
		class="org.walkmod.sonar.visitors.AddSwitchDefaultCase"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:ArrayDesignatorOnType"
		class="org.walkmod.sonar.visitors.ArrayDesignatorOnType"></bean>

	<bean
		id="org.walkmod:walkmod-sonar-plugin:PrimitiveInstantiationForToString"
		class="org.walkmod.sonar.visitors.PrimitiveInstantiationForToString"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveCodeComment"
		class="org.walkmod.sonar.visitors.RemoveCodeComment"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveEmptyMethod"
		class="org.walkmod.sonar.visitors.RemoveEmptyMethod"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveEmptyStatement"
		class="org.walkmod.sonar.visitors.RemoveEmptyStatement"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveLiteralBoolean"
		class="org.walkmod.sonar.visitors.RemoveLiteralBoolean"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:StaticInitializedFieldToFinal"
		class="org.walkmod.sonar.visitors.StaticInitializedFieldToFinal"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:StringCheckOnLeft"
		class="org.walkmod.sonar.visitors.StringCheckOnLeft"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:UseCollectionIsEmpty"
		class="org.walkmod.sonar.visitors.UseCollectionIsEmpty"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:UseStringEquals" class="org.walkmod.sonar.visitors.UseStringEquals"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:VariableImmediatelyReturned"
		class="org.walkmod.sonar.visitors.VariableImmediatelyReturned"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:CollapsibleIfStatements"
		class="org.walkmod.sonar.visitors.CollapsibleIfStatements"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RedundantCastsShouldNotBeUsed"
		class="org.walkmod.sonar.visitors.RedundantCastsShouldNotBeUsed"></bean>

	<bean
		id="org.walkmod:walkmod-sonar-plugin:DeclarationsShouldUseCollectionInterfaces"
		class="org.walkmod.sonar.visitors.DeclarationsShouldUseCollectionInterfaces"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveUselessParentheses"
		class="org.walkmod.sonar.visitors.RemoveUselessParentheses"></bean>

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveUselessImports"
		class="org.walkmod.sonar.visitors.RemoveUselessImports" />

	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveUnusedMethodParameters"
		class="org.walkmod.sonar.visitors.RemoveUnusedMethodParameters"></bean>
	
	<bean id="org.walkmod:walkmod-sonar-plugin:RemoveUselessVariables"
        class="org.walkmod.sonar.visitors.RemoveUselessVariables"></bean>
        
    <bean id="org.walkmod:walkmod-sonar-plugin:LocalVarsShouldComplyWithNamingConvention"
        class="org.walkmod.sonar.visitors.LocalVarsShouldComplyWithNamingConvention"></bean>

</beans>
